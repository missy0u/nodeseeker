{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nodeseeker",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": [
		"nodejs_compat"
	],
  "main": "./src/index.tsx",
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nodeseeker",
      "database_id": "f54f6f59-e35d-4965-893e-2091296f691d"
    }
  ],
  "triggers": {
    "crons": [
      "*/1 * * * *"
    ]
  },
  "observability": {
    "enabled": false
  },
	"assets": {
		"directory": "./dist",
		"not_found_handling": "single-page-application"
	},
}
